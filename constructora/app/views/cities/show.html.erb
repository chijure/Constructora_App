<div class="jumbotron p-4">
  <h2><b>Ver | Ubigeo - Departamento</b></h2>
	<div class="table-responsive-sm w-50">
		<h5>
			<p id="notice" class="badge badge-pill badge-success pt-2 pb-2"><%= notice %></p>
		</h5>
		<p>
		  <strong>Departamento:</strong>
		  <td><%= State.select(:Name).where("id = " + @city.state_id.to_s).collect(&:Name)[0] %></td>
		</p>

		<p>
		  <strong>Name:</strong>
		  <%= @city.Name %>
		</p>

		<%= link_to image_tag('EditIconBig.png'), edit_city_path(@city), title: 'Editar' %> |
		<%= link_to image_tag('BackIconBig.png'), cities_path, title: 'Atras'  %>

	</div>
</div>
