<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Idclient</th>
      <th>Idcity</th>
      <th>Identitynumber</th>
      <th>Name</th>
      <th>Lastname</th>
      <th>Address</th>
      <th>Mail</th>
      <th>Phone</th>
      <th>Isactive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.IdClient %></td>
        <td><%= client.IdCity %></td>
        <td><%= client.IdentityNumber %></td>
        <td><%= client.Name %></td>
        <td><%= client.LastName %></td>
        <td><%= client.Address %></td>
        <td><%= client.Mail %></td>
        <td><%= client.Phone %></td>
        <td><%= client.IsActive %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
