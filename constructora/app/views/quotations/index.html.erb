<p id="notice"><%= notice %></p>

<h1>Quotations</h1>

<table>
  <thead>
    <tr>
      <th>Idquotation</th>
      <th>Idrequestquotation</th>
      <th>Quotationdate</th>
      <th>Price</th>
      <th>Validfor</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations.each do |quotation| %>
      <tr>
        <td><%= quotation.IdQuotation %></td>
        <td><%= quotation.IdRequestQuotation %></td>
        <td><%= quotation.QuotationDate %></td>
        <td><%= quotation.Price %></td>
        <td><%= quotation.ValidFor %></td>
        <td><%= quotation.Status %></td>
        <td><%= link_to 'Show', quotation %></td>
        <td><%= link_to 'Edit', edit_quotation_path(quotation) %></td>
        <td><%= link_to 'Destroy', quotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quotation', new_quotation_path %>
